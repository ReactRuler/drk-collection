<header class="site-header">
  <div class="page-width">
    <div class="header-wrapper">
      <div class="header-logo">
        <a href="{{ section.settings.logo_link | default: '/' }}">
          {% if section.settings.logo %}
            <img src="{{ section.settings.logo | image_url: width: 200 }}" alt="{{ section.settings.logo.alt | default: 'DRK Collection' }}">
          {% else %}
            <img src="{{ 'drk-logo.png' | asset_url }}" alt="DRK Collection">
          {% endif %}
        </a>
      </div>
      <nav class="header-nav">
        {% if section.blocks.size > 0 %}
          {% for block in section.blocks %}
            {% if block.type == 'menu_item' %}
              <div class="menu-item-wrapper" data-menu-item>
                <a href="{{ block.settings.link | default: '#' }}" class="menu-link">{{ block.settings.label }}</a>
                <div class="menu-dropdown" style="display: none;" data-dropdown>
                <div class="dropdown-content">
                  {% if block.settings.featured_product_image %}
                    <div class="dropdown-featured">
                      <div class="featured-product-image">
                        <img src="{{ block.settings.featured_product_image | image_url: width: 300 }}" alt="{{ block.settings.featured_product_title }}">
                      </div>
                      <div class="featured-product-info">
                        <h4>{{ block.settings.featured_product_title | default: 'PRODUCT NAME' }}</h4>
                        {% if block.settings.featured_product_original_price != blank or block.settings.featured_product_sale_price != blank %}
                          <div class="featured-product-price">
                            {% if block.settings.featured_product_original_price != blank %}
                              <span class="original-price">{{ block.settings.featured_product_original_price }}</span>
                            {% endif %}
                            {% if block.settings.featured_product_sale_price != blank %}
                              <span class="sale-price">{{ block.settings.featured_product_sale_price }}</span>
                            {% endif %}
                          </div>
                        {% endif %}
                      </div>
                    </div>
                  {% endif %}
                  <div class="dropdown-menu-columns">
                    {% comment %} First, check for submenu blocks that match this menu item {% endcomment %}
                    {% assign found_submenu = false %}
                    {% for submenu_block in section.blocks %}
                      {% if submenu_block.type == 'submenu' and submenu_block.settings.parent_menu_label == block.settings.label %}
                        {% assign found_submenu = true %}
                        <div class="dropdown-column">
                          {% for i in (1..10) %}
                            {% assign label_key = 'menu_item_' | append: i | append: '_label' %}
                            {% assign link_key = 'menu_item_' | append: i | append: '_link' %}
                            {% if submenu_block.settings[label_key] != blank %}
                              <a href="{{ submenu_block.settings[link_key] | default: '#' }}" class="dropdown-link">{{ submenu_block.settings[label_key] }}</a>
                            {% endif %}
                          {% endfor %}
                        </div>
                      {% endif %}
                    {% endfor %}
                    
                    {% comment %} Fallback to inline submenu links if no submenu block found {% endcomment %}
                    {% unless found_submenu %}
                      {% assign has_links = false %}
                      {% for i in (1..10) %}
                        {% assign label_key = 'submenu_link_' | append: i | append: '_label' %}
                        {% if block.settings[label_key] != blank %}
                          {% assign has_links = true %}
                          {% break %}
                        {% endif %}
                      {% endfor %}
                      {% if has_links %}
                        <div class="dropdown-column">
                          {% for i in (1..10) %}
                            {% assign label_key = 'submenu_link_' | append: i | append: '_label' %}
                            {% assign link_key = 'submenu_link_' | append: i | append: '_link' %}
                            {% if block.settings[label_key] != blank %}
                              <a href="{{ block.settings[link_key] | default: '#' }}" class="dropdown-link">{{ block.settings[label_key] }}</a>
                            {% endif %}
                          {% endfor %}
                        </div>
                      {% endif %}
                    {% endunless %}
                  </div>
                </div>
              </div>
            </div>
          {% endif %}
        {% endfor %}
        {% else %}
          {% comment %} Fallback menu if no blocks configured {% endcomment %}
          <div class="menu-item-wrapper" data-menu-item>
            <a href="#" class="menu-link">DRINKWARE</a>
            <div class="menu-dropdown" style="display: none;" data-dropdown>
              <div class="dropdown-content">
                <div class="dropdown-menu-columns">
                  <div class="dropdown-column">
                    <a href="#" class="dropdown-link">Water Bottles</a>
                    <a href="#" class="dropdown-link">Travel Mugs</a>
                    <a href="#" class="dropdown-link">Tumblers</a>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div class="menu-item-wrapper" data-menu-item>
            <a href="#" class="menu-link">FITNESS</a>
            <div class="menu-dropdown" style="display: none;" data-dropdown>
              <div class="dropdown-content">
                <div class="dropdown-menu-columns">
                  <div class="dropdown-column">
                    <a href="#" class="dropdown-link">Weights</a>
                    <a href="#" class="dropdown-link">Yoga Mats</a>
                    <a href="#" class="dropdown-link">Equipment</a>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div class="menu-item-wrapper" data-menu-item>
            <a href="#" class="menu-link">HOME</a>
            <div class="menu-dropdown" style="display: none;" data-dropdown>
              <div class="dropdown-content">
