            <div class="menu-dropdown" style="display: none;" data-dropdown>
              <div class="dropdown-content">
                <div class="dropdown-menu-columns">
                  <div class="dropdown-column">
                    <a href="#" class="dropdown-link">Luggage</a>
                    <a href="#" class="dropdown-link">Bags</a>
                    <a href="#" class="dropdown-link">Accessories</a>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div class="menu-item-wrapper" data-menu-item>
            <a href="#" class="menu-link">BASICS</a>
            <div class="menu-dropdown" style="display: none;" data-dropdown>
              <div class="dropdown-content">
                <div class="dropdown-menu-columns">
                  <div class="dropdown-column">
                    <a href="#" class="dropdown-link">T-Shirts</a>
                    <a href="#" class="dropdown-link">Hoodies</a>
                    <a href="#" class="dropdown-link">Pants</a>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div class="menu-item-wrapper" data-menu-item>
            <a href="#" class="menu-link">DIGITAL</a>
            <div class="menu-dropdown" style="display: none;" data-dropdown>
              <div class="dropdown-content">
                <div class="dropdown-menu-columns">
                  <div class="dropdown-column">
                    <a href="#" class="dropdown-link">Headphones</a>
                    <a href="#" class="dropdown-link">Speakers</a>
                    <a href="#" class="dropdown-link">Accessories</a>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div class="menu-item-wrapper" data-menu-item>
            <a href="#" class="menu-link">OFFICE</a>
            <div class="menu-dropdown" style="display: none;" data-dropdown>
              <div class="dropdown-content">
                <div class="dropdown-menu-columns">
                  <div class="dropdown-column">
                    <a href="#" class="dropdown-link">Desk Accessories</a>
                    <a href="#" class="dropdown-link">Organizers</a>
                    <a href="#" class="dropdown-link">Supplies</a>
                  </div>
                </div>
              </div>
            </div>
          </div>
        {% endif %}
      </nav>
      <div class="header-icons">
        <button class="search-icon" onclick="document.getElementById('search-popup').style.display='flex'">
          <svg width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="1.5">
            <circle cx="11" cy="11" r="8" />
            <path d="m21 21-4.35-4.35" />
          </svg>
        </button>
        <button class="cart-icon" onclick="openCartDrawer()">
          <svg width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="1.5">
            <circle cx="9" cy="21" r="2.25" />
            <circle cx="20" cy="21" r="2.25" />
            <path d="M1 1h4l2.68 13.39a2 2 0 0 0 2 1.61h9.72a2 2 0 0 0 2-1.61L23 6H6" />
          </svg>
          <span class="cart-count" id="cart-count">0</span>
        </button>
        <div class="account-menu-wrapper">
          <button class="account-icon" onclick="toggleAccountDropdown()">
            {% if section.settings.account_icon_image %}
              <img src="{{ section.settings.account_icon_image | image_url: width: 40 }}" alt="Account">
            {% else %}
              <svg width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="1.5">
                <circle cx="12" cy="7" r="3" />
                <path d="M20 21v-2a4 4 0 0 0-4-4H8a4 4 0 0 0-4 4v2" />
              </svg>
            {% endif %}
          </button>
          <div id="account-dropdown" class="account-dropdown">
            {% for account_item in section.blocks %}
              {% if account_item.type == 'account_menu_item' %}
                <a href="{{ account_item.settings.link | default: '#' }}" class="account-menu-link">{{ account_item.settings.label }}</a>
              {% endif %}
            {% endfor %}
            <div class="account-buttons">
              <a href="{{ section.settings.login_link | default: '/account/login' }}" class="account-button login-button">{{ section.settings.login_text | default: 'LOGIN' }}</a>
              <a href="{{ section.settings.register_link | default: '/account/register' }}" class="account-button register-button">{{ section.settings.register_text | default: 'REGISTER' }}</a>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</header>

<div id="search-popup" class="search-popup" style="display: none;" onclick="if(event.target === this) this.style.display='none'">
  <div class="search-popup-content" onclick="event.stopPropagation()">
    <button class="search-close" onclick="document.getElementById('search-popup').style.display='none'">√ó</button>
    <form class="search-form" action="/search" method="get">
      <input type="search" name="q" placeholder="Search products, articles, pages..." autofocus>
      <button type="submit">Search</button>
    </form>
  </div>
</div>

<div id="cart-drawer-overlay" class="cart-drawer-overlay" onclick="closeCartDrawer()" style="display: none;"></div>
<div id="cart-drawer" class="cart-drawer">
  <div class="cart-drawer-content">
    <div class="cart-drawer-header">
      <h2>Review Your Cart (<span id="cart-item-count">0</span>)</h2>
      <button class="cart-close" onclick="closeCartDrawer()">√ó</button>
    </div>
    <div class="cart-drawer-items" id="cart-items">
      <!-- Cart items will be loaded here -->
    </div>
    <div class="cart-drawer-summary">
      <div class="cart-summary-row">
        <span>Subtotal</span>
        <span id="cart-subtotal">$ 0</span>
      </div>
      <div class="cart-summary-row">
        <span>Shipping</span>
        <span id="cart-shipping">$ {{ section.settings.shipping_cost | default: '15' }}</span>
      </div>
      <div class="cart-summary-row total">
        <span>Total</span>
        <span id="cart-total">$ 0</span>
      </div>
    </div>
    <div class="cart-drawer-actions">
      <button class="cart-button update-cart" onclick="updateCart()">UPDATE CART</button>
      <a href="/checkout" class="cart-button checkout-button">PROCEED TO CHECKOUT</a>
    </div>
  </div>
</div>

<script>
  (function() {
    'use strict';
  
  window.openCartDrawer = function() {
    const drawer = document.getElementById('cart-drawer');
    const overlay = document.getElementById('cart-drawer-overlay');
    if (drawer && overlay) {
      drawer.classList.add('active');
      overlay.style.display = 'block';
      updateCartCount();
    }
  };
  
  window.closeCartDrawer = function() {
    const drawer = document.getElementById('cart-drawer');
    const overlay = document.getElementById('cart-drawer-overlay');
    if (drawer && overlay) {
      drawer.classList.remove('active');
      overlay.style.display = 'none';
    }
  };
  
  window.toggleAccountDropdown = function() {
    const dropdown = document.getElementById('account-dropdown');
    if (dropdown) {
      dropdown.classList.toggle('active');
    }
  };
  
  function updateCartCount() {
    fetch('/cart.js')
      .then(response => response.json())
      .then(cart => {
        const cartCount = document.getElementById('cart-count');
        const cartItemCount = document.getElementById('cart-item-count');
        const cartSubtotal = document.getElementById('cart-subtotal');
        const cartTotal = document.getElementById('cart-total');
        
        if (cartCount) cartCount.textContent = cart.item_count;
        if (cartItemCount) cartItemCount.textContent = cart.item_count;
        if (cartSubtotal) cartSubtotal.textContent = '$ ' + (cart.total_price / 100).toFixed(0);
        
        const shipping = parseFloat('{{ section.settings.shipping_cost | default: 15 }}') || 15;
        const total = (cart.total_price / 100) + shipping;
        if (cartTotal) cartTotal.textContent = '$ ' + total.toFixed(0);
        
        const cartItemsContainer = document.getElementById('cart-items');
        if (cartItemsContainer) {
          if (cart.items && cart.items.length > 0) {
            cartItemsContainer.innerHTML = cart.items.map(item => `
              <div class="cart-item">
                <div class="cart-item-image">
                  <img src="${item.image || ''}" alt="${item.title || 'Product'}">
                </div>
                <div class="cart-item-info">
                  <h4 class="cart-item-title">${item.title || 'Product Name'}</h4>
                  <p class="cart-item-variant">Size: ${item.variant_title || 'L'}</p>
                  <button class="cart-item-remove" onclick="removeFromCart(${item.id})">üóëÔ∏è</button>
                </div>
              </div>
            `).join('');
          } else {
            cartItemsContainer.innerHTML = '<p style="color: #767676; text-align: center; padding: 40px;">Your cart is empty</p>';
          }
        }
      })
      .catch(error => {
        console.error('Error fetching cart:', error);
      });
  }
  
  window.removeFromCart = function(itemId) {
    fetch('/cart/change.js', {
      method: 'POST',
      headers: { 'Content-Type': 'application/json' },
      body: JSON.stringify({ id: itemId, quantity: 0 })
    })
    .then(() => updateCartCount());
  };
  
  window.updateCart = function() {
    updateCartCount();
  };
  
  function initMenuDropdowns() {
    const menuItems = document.querySelectorAll('.menu-item-wrapper[data-menu-item]');
    
    menuItems.forEach(function(menuItem) {
      const dropdown = menuItem.querySelector('.menu-dropdown[data-dropdown]');
      if (!dropdown) return;
      
      let hideTimeout;
      
      function showDropdown() {
        clearTimeout(hideTimeout);
        dropdown.style.display = 'block';
      }
      
      function hideDropdown() {
        clearTimeout(hideTimeout);
        hideTimeout = setTimeout(function() {
          if (!menuItem.matches(':hover') && !dropdown.matches(':hover')) {
            dropdown.style.display = 'none';
          }
        }, 150);
      }
      
      menuItem.addEventListener('mouseenter', showDropdown);
      menuItem.addEventListener('mouseleave', hideDropdown);
      dropdown.addEventListener('mouseenter', showDropdown);
      dropdown.addEventListener('mouseleave', hideDropdown);
    });
  }
  
  // Make it globally accessible
  window.initMenuDropdowns = initMenuDropdowns;
  
  window.showDropdown = function(element) {
    const dropdown = element.querySelector('.menu-dropdown');
    if (dropdown) {
      dropdown.style.display = 'block';
    }
  };
  
  window.hideDropdownDelayed = function(element) {
    if (!element) return;
    const dropdown = element.querySelector('.menu-dropdown');
    if (dropdown) {
      setTimeout(function() {
        if (dropdown && !dropdown.matches(':hover') && !element.querySelector('.menu-link').matches(':hover')) {
          dropdown.style.display = 'none';
        }
      }, 100);
    }
  };
  
  window.hideDropdown = function(element) {
    const dropdown = element.querySelector('.menu-dropdown');
    if (dropdown) {
      dropdown.style.display = 'none';
    }
  };
  
  function initAll() {
    updateCartCount();
    initMenuDropdowns();
    
    // Close cart when clicking overlay
    const overlay = document.getElementById('cart-drawer-overlay');
    if (overlay) {
      overlay.addEventListener('click', closeCartDrawer);
    }
    
    // Close cart when clicking close button
    const closeBtn = document.querySelector('.cart-close');
    if (closeBtn) {
      closeBtn.addEventListener('click', closeCartDrawer);
    }
    
    // Close account dropdown when clicking outside
    document.addEventListener('click', function(event) {
      const accountWrapper = document.querySelector('.account-menu-wrapper');
      const accountDropdown = document.getElementById('account-dropdown');
      if (accountWrapper && accountDropdown && !accountWrapper.contains(event.target)) {
        accountDropdown.classList.remove('active');
      }
    });
  }
  
  if (document.readyState === 'loading') {
    document.addEventListener('DOMContentLoaded', initAll);
  } else {
    initAll();
  }
  
  // Also initialize on theme editor updates
  if (typeof window.Shopify !== 'undefined' && window.Shopify.designMode) {
    document.addEventListener('shopify:section:load', initAll);
    document.addEventListener('shopify:section:reorder', initAll);
    document.addEventListener('shopify:section:select', initAll);
  }
  
  // Re-initialize when section is reloaded
  document.addEventListener('shopify:section:load', function(event) {
    if (event.detail.sectionId === '{{ section.id }}') {
      setTimeout(initAll, 100);
    }
  });
</script>

